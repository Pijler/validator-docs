{
    "name": "jp-system/validator-docs",
    "type": "library",
    "description": "This package provides validations for documents in Brazil",
    "keywords": [
        "docs",
        "laravel",
        "validator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jo√£o Pedro Lopes",
            "email": "joaopalopes24@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "orchestra/testbench": "^10.4",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "ValidatorDocs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ValidatorDocs\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable"
}
